      if      ( mode = yasic.mInvalid )                                         ;< invalid >
        mode                            yasic_fail                              'probably undefined mode!',                                 char,     line,     go,       pos,      input
      else if ( mode = yasic.mHeader )                                          ;< header >
        load                            head byte                               from header:( ipos )
