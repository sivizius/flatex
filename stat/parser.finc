          end if
          mode                          =                                       yasic.mInlineOp
        end if
      else if ( mode = yasic.mInlineRemark )                                    ;< inline remark >
        if      ( char = yasic.sNewLine )
          db                            yasic.sNewLine
          offs                          =                                       null
          mode                          =                                       yasic.mInlineParse
        else if ( char = '\' )
          class                         =                                       yasic.mInlineRemark
          mode                          =                                       yasic.mInlineEscape
        else
          offs                          =                                       ( offs + isize )
          repeat                        ( isize )
            load                        char byte                               from utf8buf:( % - 1 )
            db                          char
          end repeat
        end if
      else if ( mode = yasic.mInlineVar )                                       ;< inline var >
        if (( char >= 'A' & char <= 'Z' ) | ( char >= 'a' & char <= 'z' ))
          func                          __hash_char__                           func,                                   ( char or yasic.fCaseMask )
        else
          func                          __hash_char__                           func,                                   0
          func                          __hash_char__                           func,                                   0
          func                          __hash_char__                           func,                                   0
          func                          __hash_char__                           func,                                   0
          func                          __hash_char__                           func,                                   0
          func                          __hash_char__                           func,                                   0
          func                          __hash_char__                           func,                                   0
          if      ( func = yasic.lChar )
            db                          yasic.TheChar
            offs                        =                                       ( offs + yasic.TheChar.size )
          else
            mode                        yasic_fail                              'unknown symbol',                                           char,     line,     go,       pos,      input
          end if

          if ( char = '\' )
            if ( class = yasic.mInlineWait )
              class                     =                                       yasic.mInlineArg
            end if
            mode                        =                                       yasic.mInlineEscape
          else if ( char = '$' )
            mode                        =                                       class
          else if ( char = yasic.sApostrophe )
            offs                        =                                       ( offs + 1 )
            db                          yasic.sApostrophe
            mode                        =                                       yasic.mInlineString
          else if ( char = '(' | char = '[' | char = '{' )
            offs                        =                                       ( offs + 1 )
            db                          char
            inline.brackets             =                                       ( inline.brackets + 1 )
            mode                        =                                       yasic.mInlineArg
          else if ( char = ')' | char = ']' | char = '}' )
            offs                        =                                       ( offs + 1 )
            db                          char
            inline.brackets             =                                       ( inline.brackets - 1 )
            mode                        =                                       yasic.mInlineArg
          else if ( inline.brackets = 0 & char = ' ' )
            if ( offs < 41 )
              times ( 41 - offs )       db                                      ' '
            else
              db '§0x'
              hq offs
              db '§'
            end if
            offs                        =                                       ( 1 )
            mode                        =                                       yasic.mInlineWait
          else if ( class = yasic.mInlineArg & inline.brackets = 0 & char = ',' )
            db                          ','
            times ( 40 - offs )         db                                      ' '
            offs                        =                                       ( 1 )
            mode                        =                                       yasic.mInlineWait
          else if ( char = ';' )
            db                          ';'
            offs                        =                                       ( offs + 1 )
            mode                        =                                       yasic.mInlineRemark
          else if ( char = yasic.sNewLine )
            offs                        =                                       null
            mode                        =                                       yasic.mInlineParse
            db                          yasic.sNewLine
          else
            offs                        =                                       ( offs + isize )
            repeat                      ( isize )
              load                      char byte                               from utf8buf:( % - 1 )
              db                        char
            end repeat
            mode                        =                                       class
          end if
        end if
      else if ( mode = yasic.mInlineWait )                                      ;< inline wait >
        if      ( char = ' ' )
        else if ( char = '\' )
          class                         =                                       yasic.mInlineArg
          mode                          =                                       yasic.mInlineEscape
        else if ( char = '$' )
          func                          =                                       yasic.label_seed
          class                         =                                       yasic.mInlineArg
          mode                          =                                       yasic.mInlineVar
        else if ( char = yasic.sApostrophe )
          offs                          =                                       ( offs + 1 )
          db                            yasic.sApostrophe
          mode                          =                                       yasic.mInlineString
        else if ( char = '(' | char = '[' | char = '{' )
          offs                          =                                       ( offs + 1 )
          db                            char
          inline.brackets               =                                       ( inline.brackets + 1 )
          mode                          =                                       yasic.mInlineArg
        else if ( char = ')' | char = ']' | char = '}')
          offs                          =                                       ( offs + 1 )
          db                            char
          inline.brackets               =                                       ( inline.brackets - 1 )
          mode                          =                                       yasic.mInlineArg
        else if ( char = ';' )
          db                            ';'
          offs                          =                                       ( offs + 1 )
          mode                          =                                       yasic.mInlineRemark
        else if ( char = yasic.sNewLine )
          offs                          =                                       null
          mode                          =                                       yasic.mInlineParse
          db                            yasic.sNewLine
        else
          offs                          =                                       ( offs + isize )
          repeat                        ( isize )
            load                        char byte                               from utf8buf:( % - 1 )
            db                          char
          end repeat
          mode                          =                                       yasic.mInlineArg
