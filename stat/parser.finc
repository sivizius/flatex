    idepth                              =                                       ( 0 )
  end if
  textInserted                          =                                       null
  ichar                                 =                                       null
  char                                  =                                       null
  line                                  =                                       ( 1 )
  ipos                                  =                                       null
  isize                                 =                                       null
  pos                                   =                                       ( 0 - 1 )
  utf8                                  =                                       null
  group                                 =                                       null
  group.ctr                             =                                       null
  mode                                  =                                       yasic.mHeader
  value                                 =                                       header.size
  depth                                 =                                       idepth
  class                                 =                                       yasic.class_seed
  function                              =                                       ( 1 )
  begin                                 =                                       ( 1 )
  mode                                  =                                       yasic.mHeader
  value                                 =                                       header.size
  depth                                 =                                       idepth
  xdepth                                =                                       null
  class                                 =                                       yasic.class_seed
  count                                 =                                       null
  go                                    =                                       null
  message.ctr                           =                                       null
  output.offs                           =                                       null
  repeat                                chars.size
    if ( group = 65536 )
