    db                                  ';( structure )', yasic.sNewLine
    times ( idepth )                    db                                      '  '
    db                                  ';{', yasic.sNewLine
    times ( idepth )                    db                                      '  '
    db                                  ';:  '
    temp                                =                                       1
    repeat                              ( output.offs )
      load                              char byte                               from output:( % - 1 )
      if      ( char = yasic.sNull    )
        db                              ' '
        display                         ' '
        temp                            =                                       0
      else if ( char = yasic.sNewLine )
        if ( temp = 0 )
          db                            yasic.sNewLine
          times ( idepth )              db                                      '  '
          db                            ';:  '
        end if
        display                         yasic.sNewLine
        temp                            =                                       1
      else
        db                              char
        display                         char
        temp                            =                                       0
      end if
    end repeat
    times ( idepth )                    db                                      '  '
    db                                  yasic.sNewLine, ';}', yasic.sNewLine
  else if ( mode = yasic.mError )
    db                                  yasic.sNewLine
    db                                  ';<[asm:yasic] script stopped lethally!>'
    db                                  yasic.sNewLine
  else
    mode                                yasic_fail                              'unexpected end of script!',                                char,     line,     go,       pos,      input
  end if
}