    else if ( ichar >= 0xe0 & utf8 =    null )
      store                             byte ichar                              at utf8buf:( 0 )
      utf8                              =                                       ( 2 )
      isize                             =                                       ( 3 )
      char                              =                                       ( ichar and 0x0f )
    else if ( ichar >= 0xc0 & utf8 =    null )
      store                             byte ichar                              at utf8buf:( 0 )
