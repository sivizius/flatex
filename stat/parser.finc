    end if
    if ( group.ctr = 65536 )
      mode                              yasic_fail                              'too many options in group!',                               char,     line,     go,       pos,      input
    end if
    if ( ichar = yasic.sNewLine )
      line                              =                                       ( line + 1 )
      go                                =                                       ( ipos )
      pos                               =                                       ( null )
    else
      pos                               =                                       ( pos + 1 )
    end if
    load                                ichar byte                              from chars:( % - 1 )
